{
  "family": "PropertyAppTask",
  "requiresCompatibilities": ["EC2"],
  "memory": "900",
  "cpu": "1024",

  "containerDefinitions": [
    {
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "memoryReservation": 200,
      "image": "823955147616.dkr.ecr.ap-southeast-2.amazonaws.com/property-app-nginx:latest",
      "name": "property-app-nginx"
    },
    {
      "memoryReservation": 300,
      "image": "823955147616.dkr.ecr.ap-southeast-2.amazonaws.com/property-app-client:latest",
      "name": "property-app-client"
    },
    {
      "memoryReservation": 400,
      "image": "823955147616.dkr.ecr.ap-southeast-2.amazonaws.com/property-app-backend:latest",
      "name": "property-app-backend"
    }
  ]
}
